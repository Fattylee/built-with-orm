{
  "name": "orm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/bundle",
    "dev": "cross-env DEBUG=active nodemon --exec \"../node_modules/.bin/babel-node server/src/index.js\" -w server",
    "bundle": "../node_modules/.bin/babel server/src -d server/bundle && \"yarn run start\"",
    "trans:client": "../node_modules/.bin/babel client/ -d public/js --verbose -w",
    "live-server": "live-server public",
    "client": "concurrently \"yarn run trans:client\" \"yarn run live-server\"",
    "full": "concurrently \"yarn run dev\" \"yarn run trans:client\" \"yarn run live-server\" \"yarn run browserify\"",
    "browserify": "nodemon --exec \"browserify public/js/index.js -o public/js/bundle.js \" -w public/js/index.js"
  },
  "dependencies": {
    "@babel/runtime": "@latest",
    "@babel/plugin-transform-runtime": "@latest",
    "cors": "@latest",
    "debug": "@latest",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "pg": "^7.14.0",
    "sequelize": "^5.21.3",
    "volleyball": "^1.5.1"
  },
