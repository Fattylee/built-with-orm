{
  "name": "orm",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -w -r @babel/register",
    "test:jest": "cross-env NODE_ENV=test jest --verbose --watchAll",
    "start": "node server/bundle",
    "dev": "cross-env DEBUG=active nodemon --exec \"../node_modules/.bin/babel-node server/src/server.js\" -w server",
    "bundle": "../node_modules/.bin/babel server/src -d server/bundle && \"npm run start\"",
    "trans:client": "../node_modules/.bin/babel client/ -d public/js --verbose -w",
    "live-server": "live-server public",
    "client": "concurrently \"npm run trans:client\" \"npm run live-server\"",
    "full": "concurrently \"npm run dev\" \"npm run trans:client\" \"npm run live-server\" \"npm run browserify\"",
    "browserify": "nodemon --exec \"browserify public/js/index.js -o public/js/bundle.js \" -w public/js/index.js"
  },
  "dependencies": {
    "@babel/runtime": "@latest",
    "@babel/plugin-transform-runtime": "@latest",
    "chai": "^4.2.0",
    "cors": "2.8.5",
    "cross-env": "^6.0.3",
    "debug": "@latest",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "pg": "^7.14.0",
    "supertest": "^4.0.2",
    "sequelize": "^5.21.3",
    "volleyball": "^1.5.1"
  },
  "keywords": [
    "js",
    "eslint",
    "mocha",
    "houndci"
  ],
  "author": "Fatai Balogun",
  "license": "ISC"
}
